{
    "name": "pixelcode/download-form",
    "description": "Show a download form to download a custom file.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "download form"
    ],
    "support": {
        "docs": "https://github.com/PixelCodeNL/download-form/blob/master/README.md",
        "issues": "https://github.com/PixelCodeNL/download-form/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel&Code",
            "homepage": "https://www.pixelcode.nl"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "pixelcode\\downloadform\\": "src/"
        }
    },
    "extra": {
        "name": "Download Form",
        "handle": "download-form",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/PixelCodeNL/download-form/master/CHANGELOG.md",
        "components": {
            "downloadFormService": "pixelcode\\downloadform\\services\\DownloadFormService"
        },
        "class": "pixelcode\\downloadform\\DownloadForm"
    }
}
